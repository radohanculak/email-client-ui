{
    "root": true,
    "plugins": [
        "prettier"
    ],
    "extends": "airbnb-typescript",
    "parserOptions": {
        "project": "./tsconfig.json"
    },
    "rules": {
        "no-console": "warn",
        "prettier/prettier": "error",
        "react/jsx-filename-extension": "off",
        "import/extensions": "off",
        "import/no-extraneous-dependencies": "off"
    }
}
